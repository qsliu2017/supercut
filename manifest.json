{
  "manifest_version": 3,
  "name": "Supercut",
  "version": "1.0.0",
  "description": "Minimal URL Shortcut Extension",
  "omnibox": { "keyword": "s" },
  "background": { "service_worker": "background.js", "type": "module" },
  "action": {
    "default_popup": "popup/index.html",
    "default_title": "New Supercut"
  },
  "permissions": ["storage"]
}
